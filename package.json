{
  "name": "semantic-link",
  "version": "0.1.3",
  "description": "A utility library for manipulating a list of links that form a semantic interface to a resource.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clobber": "yarn run clean && rimraf ./node_modules",
    "clean": "rimraf ./lib ./coverage",
    "test": "jest --config jest.config.js",
    "test-watch": "jest --config jest.config.js --watch",
    "test-coverage": "jest --config jest.config.js --coverage",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint . --ext .js,.ts",
    "lint-fix": "eslint . --ext .js,.ts --fix",
    "prepare": "yarn run clean && yarn run build",
    "prepublishOnly": "yarn test && yarn run lint",
    "preversion": "yarn run lint",
    "version": "yarn run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/semanticlink/semanticlink-js.git"
  },
  "keywords": [
    "hypermedia",
    "microformat",
    "REST",
    "atom",
    "API"
  ],
  "author": "toddb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/semanticlink/semanticlink-js/issues"
  },
  "homepage": "https://github.com/semanticlink/semanticlink-js#readme",
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/jest": "^25.1.1",
    "@types/jest-each": "^24.3.0",
    "@types/sinon": "^7.5.0",
    "@types/underscore": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "jest-each": "^25.1.0",
    "prettier": "^1.19.1",
    "sinon": "^8.1.1",
    "ts-jest": "^25.1.0",
    "typescript": "^3.7.5",
    "underscore": "^1.9.1"
  },
  "dependencies": {
    "axios": "0.19.2",
    "bottleneck": "^2.19.5",
    "semantic-link": "git://github.com/semanticlink/semanticlink-js.git#5769ff3b09a9011ca175354ac643853dea216ef6"
  }
}
